<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.graph.AdminManageUser">
    <VBox alignment="TOP_CENTER" layoutX="328.0" layoutY="57.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <HBox alignment="CENTER" prefHeight="0.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
            <Label text="用 户 管 理">
                <font>
                    <Font size="36.0" />
                </font>
            </Label>
        </HBox>
        <VBox alignment="TOP_CENTER" prefHeight="365.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
         <TableView fx:id="tableView" prefHeight="325.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
           <columns>
             <TableColumn fx:id="idCol" minWidth="120.0" prefWidth="150.0" text="用户id" />
             <TableColumn fx:id="ownernameCol" minWidth="100.0" prefWidth="120.0" text="所有人" />
               <TableColumn fx:id="owneridCol" minWidth="220.0" prefWidth="240.0" text="所有人身份证号" />
               <TableColumn fx:id="genderCol" minWidth="20.0" prefWidth="70.0" text="性别" />
               <TableColumn fx:id="telCol" minWidth="200.0" prefWidth="220.0" text="电话号码" />
           </columns>
            <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
         </TableView>
            <HBox alignment="CENTER" prefHeight="69.0" prefWidth="800.0">
                <Button fx:id="deleteButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="79.0" text="删 除" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets right="100.0" />
                    </HBox.margin>
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Button>
            <Label fx:id="numLabel" prefHeight="40.0" prefWidth="90.0" text="用户数量：" textFill="RED">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
                <Button fx:id="addButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="80.0" text="添 加" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="100.0" />
                    </HBox.margin>
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Button>
            </HBox>
        </VBox>
    </VBox>
</AnchorPane>
