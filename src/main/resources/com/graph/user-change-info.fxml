<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" minHeight="450.0" minWidth="800.0" prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.graph.UserChangeInfo">
   <HBox alignment="CENTER_LEFT" VBox.vgrow="ALWAYS">
      <Pane prefHeight="53.0" prefWidth="0.0" HBox.hgrow="ALWAYS"/>
      <Button onAction="#returnButtonClick" prefHeight="48.0" prefWidth="123.0" text="返回">
         <font>
            <Font name="System Bold" size="24.0"/>
         </font>
      </Button>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="53.0" prefWidth="514.0" text="修改用户信息"
             textAlignment="CENTER">
         <font>
            <Font size="36.0"/>
         </font>
      </Label>
      <Pane prefHeight="53.0" prefWidth="78.0" HBox.hgrow="ALWAYS"/>
   </HBox>
   <GridPane alignment="CENTER" prefHeight="398.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="395.33331298828125" minWidth="10.0" prefWidth="215.33329264322916" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="585.3333740234375" minWidth="10.0" prefWidth="585.3333740234375" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints maxHeight="84.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
         <RowConstraints maxHeight="75.0" minHeight="10.0" prefHeight="69.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS">
         <Button fx:id="modifyButton" alignment="CENTER" contentDisplay="CENTER" onAction="#setModifyButtonClick" prefHeight="60.0" prefWidth="130.0" text="修   改" textAlignment="CENTER" HBox.hgrow="ALWAYS">
            <font>
               <Font size="18.0" />
            </font>
            <HBox.margin>
               <Insets left="20.0" />
            </HBox.margin>
         </Button>
         <Pane prefHeight="97.0" prefWidth="256.0" HBox.hgrow="ALWAYS" />
         <Button alignment="CENTER" contentDisplay="CENTER" onAction="#clearButtonClick" prefHeight="60.0" prefWidth="130.0" text="重   置" textAlignment="CENTER" HBox.hgrow="ALWAYS">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <Pane prefHeight="97.0" prefWidth="153.0" HBox.hgrow="ALWAYS" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.vgrow="ALWAYS">
         <Label text="身份证号">
            <font>
               <Font name="System Bold" size="24.0" />
            </font>
         </Label>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2" GridPane.vgrow="ALWAYS">
         <Label text="性别">
            <font>
               <Font name="System Bold" size="24.0" />
            </font>
         </Label>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
         <Label fx:id="idLabel" text="Label" textFill="#14bac3">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <Label fx:id="nameLabel" text="Label" textFill="#14bac3">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <Label fx:id="genderLabel" text="Label" textFill="#14bac3">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
         <Label text="姓名">
            <font>
               <Font name="System Bold" size="24.0" />
            </font>
         </Label>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS">
         <Label text="手机号">
            <font>
               <Font name="System Bold" size="24.0" />
            </font>
         </Label>
      </HBox>
      <TextField fx:id="telText" prefHeight="23.0" prefWidth="491.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS">
         <GridPane.margin>
            <Insets left="30.0" right="50.0" />
         </GridPane.margin>
      </TextField>
   </GridPane>
</VBox>
